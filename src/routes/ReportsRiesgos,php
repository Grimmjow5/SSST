<?php
namespace Almacen\Ssst\routes;

use Almacen\Ssst\controllers\ReportsControllers;
use Flight;

class ReportsRiesgos extends Flight{
    
    function __construct()
    {
        $reports = new ReportsControllers();
         //Vista en la que se mostrara el lformulario para genera los reportes de riesgos 
    parent::route('GET /riesgos/reports',[$reports,'reports']);   
    }

}